<IfModule mod_rewrite.c>
Options -Multiviews
RewriteEngine On
#modificar RewriteBase con la ruta correcta antes del public de tu proyecto
RewriteBase /MVC/mvcframework/public 
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^(.+)$ index.php?url=$1 [QSA,L]
</IfModule> 